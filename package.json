{
  "name": "animal-crossing-amiibo-app",
  "version": "1.2.3",
  "description": "Animal Crossing amiibo cards checklist",
  "author": "Wei Kin Huang",
  "private": true,
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "dependencies": {
    "babel-core": "6.7.6",
    "babel-plugin-transform-es3-member-expression-literals": "6.5.0",
    "babel-plugin-transform-es3-property-literals": "6.5.0",
    "babel-plugin-transform-es5-property-mutators": "6.6.5",
    "babel-plugin-transform-node-env-inline": "6.5.0",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-1": "6.5.0",
    "babelify": "7.2.0",
    "brfs": "1.4.3",
    "browserify": "13.0.0",
    "browserify-shim": "3.8.12",
    "fuse.js": "2.2.0",
    "glob": "7.0.3",
    "gulp": "3.9.1",
    "gulp-clean-css": "2.0.5",
    "gulp-concat": "2.6.0",
    "gulp-less": "3.0.5",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.2.0",
    "gulp-uglify": "1.5.3",
    "gulp-util": "3.0.7",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-glob": "1.1.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "devDependencies": {
    "bower": "1.7.9",
    "closedinterval-git-hooks": "2.1.2",
    "eslint": "2.7.0",
    "gulp-cached": "1.1.0",
    "gulp-eslint": "2.0.0",
    "shelljs": "0.6.0",
    "watchify": "3.7.0"
  },
  "browserify-shim": {
    "angular": {
      "exports": "global:angular"
    },
    "ionic": {
      "exports": "global:ionic"
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-1"
          ],
          "plugins": [
            "transform-es3-member-expression-literals",
            "transform-es3-property-literals",
            "transform-es5-property-mutators",
            "transform-node-env-inline",
            "transform-object-assign"
          ]
        }
      ],
      "browserify-shim",
      "brfs"
    ]
  },
  "cordovaPreBuildGulpTasks": [
    "less",
    "js"
  ],
  "cordovaPlugins": [
    "cordova-plugin-console@1.0.3",
    "cordova-plugin-crosswalk-webview@1.7.0",
    "cordova-plugin-device@1.1.2",
    "cordova-plugin-splashscreen@3.2.2",
    "cordova-plugin-whitelist@1.2.2",
    "ionic-plugin-keyboard@2.2.0"
  ],
  "cordovaPlatforms": [
    "android@5.1.1"
  ]
}
