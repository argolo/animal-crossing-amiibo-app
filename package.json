{
  "name": "animal-crossing-amiibo-app",
  "version": "1.0.0",
  "description": "Animal Crossing amiibo cards checklist",
  "author": "Wei Kin Huang",
  "private": true,
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "babel": "5.8.23",
    "babelify": "6.3.0",
    "brfs": "1.4.1",
    "browserify": "11.2.0",
    "browserify-shim": "3.8.10",
    "fuse.js": "1.2.2",
    "glob": "5.0.15",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-less": "3.0.3",
    "gulp-minify-css": "1.2.1",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.0.4",
    "gulp-uglify": "1.4.1",
    "gulp-util": "3.0.6",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-glob": "1.1.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "devDependencies": {
    "bower": "1.5.3",
    "closedinterval-git-hooks": "2.1.0",
    "eslint": "1.6.0",
    "gulp-cached": "1.1.0",
    "gulp-eslint": "1.0.0",
    "shelljs": "0.5.3",
    "watchify": "3.4.0"
  },
  "browserify-shim": {
    "angular": {
      "exports": "global:angular"
    },
    "ionic": {
      "exports": "global:ionic"
    }
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-shim",
      "brfs"
    ]
  },
  "cordovaPreBuildGulpTasks": [
    "less",
    "js"
  ],
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard",
    {
      "id": "cordova-plugin-whitelist",
      "locator": "https://github.com/apache/cordova-plugin-whitelist.git"
    }
  ],
  "cordovaPlatforms": [
    "android"
  ]
}
