{
  "name": "animal-crossing-amiibo-app",
  "version": "1.0.3",
  "description": "Animal Crossing amiibo cards checklist",
  "author": "Wei Kin Huang",
  "private": true,
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "dependencies": {
    "babel-core": "6.4.5",
    "babel-plugin-transform-es3-member-expression-literals": "6.3.13",
    "babel-plugin-transform-es3-property-literals": "6.3.13",
    "babel-plugin-transform-es5-property-mutators": "6.3.13",
    "babel-plugin-transform-node-env-inline": "6.3.13",
    "babel-plugin-transform-object-assign": "6.3.13",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-1": "6.3.13",
    "babelify": "7.2.0",
    "brfs": "1.4.3",
    "browserify": "13.0.0",
    "browserify-shim": "3.8.12",
    "fuse.js": "1.3.1",
    "glob": "6.0.4",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-less": "3.0.5",
    "gulp-minify-css": "1.2.3",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.1.1",
    "gulp-uglify": "1.5.1",
    "gulp-util": "3.0.7",
    "less-plugin-autoprefix": "1.5.1",
    "less-plugin-glob": "1.1.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0"
  },
  "devDependencies": {
    "bower": "1.7.7",
    "closedinterval-git-hooks": "2.1.1",
    "eslint": "1.10.3",
    "gulp-cached": "1.1.0",
    "gulp-eslint": "1.1.1",
    "shelljs": "0.5.3",
    "watchify": "3.7.0"
  },
  "browserify-shim": {
    "angular": {
      "exports": "global:angular"
    },
    "ionic": {
      "exports": "global:ionic"
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-1"
          ],
          "plugins": [
            "transform-es3-member-expression-literals",
            "transform-es3-property-literals",
            "transform-es5-property-mutators",
            "transform-node-env-inline",
            "transform-object-assign"
          ]
        }
      ],
      "browserify-shim",
      "brfs"
    ]
  },
  "cordovaPreBuildGulpTasks": [
    "less",
    "js"
  ],
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "cordova-plugin-crosswalk-webview",
    "cordova-plugin-whitelist",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android"
  ]
}
